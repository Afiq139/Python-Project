   "source": [
    "def test4perfect(number):\n",
    "    divisors=[]\n",
    "\n",
    "    for d in range(1,number):\n",
    "        if number%d == 0:\n",
    "            divisors.append(d)\n",
    "    \n",
    "    return sum(divisors)==number"
   ]
  },
